{
	"core": "WordPress/WordPress#6.4.2",
	"phpVersion": "8.0",
	"plugins": [
		"."
	],
	"themes": [
		"WordPress/twentytwentythree"
	],
	"port": 8888,
	"testsPort": 8889,
	"config": {
		"WP_DEBUG": true,
		"WP_DEBUG_LOG": true,
		"WP_DEBUG_DISPLAY": true,
		"SCRIPT_DEBUG": true,
		"WP_ENVIRONMENT_TYPE": "local"
	},
	"mappings": {
		"wp-content/plugins/cta-highlights": "."
	},
	"env": {
		"development": {
			"config": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": true
			}
		},
		"tests": {
			"config": {
				"WP_DEBUG": true,
				"WP_DEBUG_LOG": false,
				"WP_TESTS_DOMAIN": "localhost",
				"WP_TESTS_EMAIL": "admin@example.org",
				"WP_TESTS_TITLE": "CTA Highlights Test Site"
			}
		}
	}
}
